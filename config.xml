<?xml version='1.0' encoding='utf-8'?>
<widget id="com.co.sscaControl" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>SSCA CONTROL</name>
    <description>
        Control de entrada y salida
    </description>
    <author email="shelvin.batista@unisoftsystem.com.co" href="http://phonegap.com">
        Shelvin Batista Batista
    </author>
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="fullscreen" value="false" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />
    <plugin name="phonegap-plugin-barcodescanner" spec="6.0.4" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.0" />
    <plugin name="cordova.plugins.diagnostic.api-22" spec="2.3.10-api-22" />
    <plugin name="org.apache.cordova.splashscreen" spec="4.0.1" />
    <plugin name="phonegap-plugin-push" spec="1.4.5" />
    <plugin name="cordova-plugin-network-information" spec="1.3.1" />
    <feature name="http://api.phonegap.com/1.0/device" />
    <feature name="debug-server" required="true">
        <param name="domain" value="http://debug.phonegap.com" />
        <param name="key" value="willoughby" />
    </feature>
    <icon src="www/img/logo_app.png" />
    <icon src="www/img/logo_app.png" />
    <icon gap:density="ldpi" gap:platform="android" src="www/img/logo_app.png" />
    <icon gap:density="mdpi" gap:platform="android" src="www/img/logo_app.png" />
    <icon gap:density="hdpi" gap:platform="android" src="www/img/logo_app.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="www/img/logo_app.png" />
    <icon gap:platform="blackberry" src="www/img/logo_app.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="www/img/logo_app.png" />
    <icon gap:platform="ios" height="57" src="www/img/logo_app.png" width="57" />
    <icon gap:platform="ios" height="72" src="www/img/logo_app.png" width="72" />
    <icon gap:platform="ios" height="114" src="www/img/logo_app.png" width="114" />
    <icon gap:platform="ios" height="144" src="www/img/logo_app.png" width="144" />
    <icon gap:platform="ios" height="180" src="www/img/logo_app.png" width="180" />
    <icon gap:platform="webos" src="www/img/logo_app.png" />
    <icon gap:platform="winphone" src="www/img/logo_app.png" />
    <icon gap:platform="winphone" gap:role="background" src="www/img/logo_app.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="www/img/Imagen.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="www/img/Imagen.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="www/img/Imagen.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="www/img/Imagen.png" />
    <gap:splash gap:platform="blackberry" src="www/img/Imagen.png" />
    <gap:splash gap:platform="ios" height="480" src="www/img/Imagen.png" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/img/Imagen.png" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/img/Imagen.png" width="768" />
    <gap:splash gap:platform="ios" height="768" src="www/img/Imagen.png" width="1024" />
    <gap:splash gap:platform="ios" height="1334" src="www/img/Imagen.png" width="750" />
    <gap:splash gap:platform="ios" height="2208" src="www/img/Imagen.png" width="1242" />
    <gap:splash gap:platform="ios" height="1242" src="www/img/Imagen.png" width="2208" />
    <gap:splash gap:platform="winphone" src="www/img/Imagen.png" />
    <access origin="*" />
    <engine name="android" spec="~5.2.2" />
</widget>
